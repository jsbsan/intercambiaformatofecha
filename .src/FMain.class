' Gambas class file

Public Sub ButtonCarga_Click()

  TextArea1.Text = File.Load("listaRutasYficheros.txt")

End

Public Sub ButtonProcesar_Click()

  Dim lista As String[]
  Dim l As String

  lista = Split(TextArea1.text, "\n")
  comun.checkOmitirLineaSindatos = CheckBoxLineasSinDatos.value
  For Each l In lista
    comun.procesa(l)
  Next

End
